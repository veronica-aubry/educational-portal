@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<MeriEducation.Models.VocabWordList>


@foreach (var list in Model)
{
    <p>List @list.WordListId</p>
    <p>@list.Word1, @list.Word2, @list.Word3, @list.Word4</p>

    <p>@Html.ActionLink("Play the Synonym Game", "GetMatches", list)</p>
    <hr/>

}

<p>@Html.ActionLink("Add a list", "AddVocabTest", "Admin")</p>